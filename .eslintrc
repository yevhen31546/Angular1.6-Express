{
  "extends": "airbnb",
  "rules": {
    "no-console": [2, { "allow": ["info", "warn", "error"] }],
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": 0
  },
  "globals": {
    "angular": true,
    "SVGInjector": true,
    "TweenMax": true,
    "TimelineMax": true,
    "TimelineLite": true,
    "CSSPlugin": true,
    "Back": true,
    "Power1": true
  }
}
